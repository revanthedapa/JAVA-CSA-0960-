package dowhile;

public class SUM 
{
	public static void main(String[] args) 
	{
		int i = 10;
		int sum=0;
		System.out.println("Countdown to Launch!");
		do 
		{
		  System.out.println(i);
		  sum=sum+i;
		  i--;
		}
		while (i >= 0);
		System.out.println("Blast Off!");
		System.out.print("Sum of numbers is "+sum+".");
	}
}
